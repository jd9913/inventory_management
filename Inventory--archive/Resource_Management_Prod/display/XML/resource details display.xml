      empty-cells:show;
      margin-bottom:0.2em;
  }
  
  table.display td{
      border-right:solid 1px;
      border-top:solid 1px;
      color:#000;
      border-color:#ccc;
      padding:0.1em 0.3em 0.1em 0.3em;
  }
  
  @media print{
    body{ background-color:#FFFFFF; background-image:none; color:#000000 }
    #ad{ display:none;}
    #leftbar{ display:none;}
    #contentarea{ width:100%;}
    .hideprint{ display:none;}
