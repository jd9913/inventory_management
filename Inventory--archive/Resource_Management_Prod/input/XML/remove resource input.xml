    <script type="text/javascript">
    var numSelect = document.getElementsByTagName("select").length;
    var elemSelect = document.getElementsByTagName("select");
    var fkTable = '<foreignkeyreference table="Inventory Request Table"></foreignkeyreference>';
    //alert(fkTable);
    for (i = 0; i &lt; numSelect; i++) {
      if (elemSelect[i].name == fkTable) {
        var optLength = elemSelect[i].options.length;
        var fkSelect = elemSelect[i];
        for (j = 0; j &lt; optLength; j++) {
          var selSplit = fkSelect.options[j].text.split("|");
          //place each iteration of the foreign key fields in their respective input;
          document.getElementById("Request_requestor_date_time").value = selSplit[1];
          document.getElementById("Request_tracking_number").value = selSplit[2];
          document.getElementById("Request_a_1Quantity").value = selSplit[3];
          document.getElementById("Request_short_description").value = selSplit[4];
          document.getElementById("Request_b_1_NIMSType").value = selSplit[5];
          document.getElementById("Request_c_1_OrderDeskType").value = selSplit[6];
          document.getElementById("Request_d_1_NIMScategory").value = selSplit[7];
          document.getElementById("Request_OrderDeskPriority").value = selSplit[8];
          document.getElementById("Request_f_1_DetailedItemDescription").value = selSplit[9];
          document.getElementById("Request_g_location_1").value = selSplit[10];
          document.getElementById("Request_f_date_time_1").value = selSplit[11];
          document.getElementById("Request_h_1").value = selSplit[12];
          document.getElementById("Request_i_1").value = selSplit[13];
          document.getElementById("Request_j_1").value = selSplit[14];
          document.getElementById("Request_requestor_5").value = selSplit[15];
          document.getElementById("Request_request_poc").value = selSplit[16];
          document.getElementById("Request_poc_number").value = selSplit[17];
          document.getElementById("Request_status").value = selSplit[18];
        }
      }
    }
  </script>
    <script type="text/javascript">
    var selResource = document.getElementById("Resource_Status");
    for (i = 0; i &lt; selResource.options.length; i++) {
      if (selResource.options[i].value == 'Available') {
        selResource.selectedIndex = i;
      }
    }
  </script>
